<p id="notice"><%= notice %></p>

<h1>Trans Categories</h1>

<table>
  <thead>
    <tr>
      <th>Transportation</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trans_categories.each do |trans_category| %>
      <tr>
        <td><%= trans_category.transportation_id %></td>
        <td><%= trans_category.category_id %></td>
        <td><%= link_to 'Show', trans_category %></td>
        <td><%= link_to 'Edit', edit_trans_category_path(trans_category) %></td>
        <td><%= link_to 'Destroy', trans_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trans Category', new_trans_category_path %>
