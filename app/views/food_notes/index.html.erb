<p id="notice"><%= notice %></p>

<h1>Food Notes</h1>

<table>
  <thead>
    <tr>
      <th>Food</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_notes.each do |food_note| %>
      <tr>
        <td><%= food_note.food_id %></td>
        <td><%= food_note.note_id %></td>
        <td><%= link_to 'Show', food_note %></td>
        <td><%= link_to 'Edit', edit_food_note_path(food_note) %></td>
        <td><%= link_to 'Destroy', food_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dest Note', new_dest_note_path %>
